{
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": ["#bfff00", "#006500"],
    "vim.statusBarColors.insert": ["#ffffff", "#0098cb"],
    "vim.statusBarColors.visual": ["#ff7f00", "#982600"],
    "vim.statusBarColors.visualline": ["#ff7f00", "#982600"],
    "vim.statusBarColors.visualblock": ["#ff7f00", "#982600"],
    "vim.statusBarColors.replace": ["#ff0000", "#ffffff"],

    "workbench.colorTheme": "Monokai",
    "C_Cpp.intelliSenseEngine": "Default",
    
    "workbench.colorCustomizations": {
        "statusBar.background": "#bfff00",
        "statusBar.noFolderBackground": "#bfff00",
        "statusBar.debuggingBackground": "#bfff00",
        "statusBar.foreground": "#006500"
    },
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.shell.windows": "C:/Program Files/Git/bin/bash.exe",
    "terminal.integrated.shell.osx": "/bin/bash",
    
    "C_Cpp.default.browse.path": [
        "${env:PATH}",
        "${workspaceFolder}/**",
    ],
    "C_Cpp.default.includePath": [
        "${env:PATH}",
        "${workspaceFolder}/**",
    ],
    
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
}
